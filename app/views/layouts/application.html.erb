<!DOCTYPE html>
<html>
  <head>
    <title>RailsWebpackerVue</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%#= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%#= javascript_include_tag Webpacker::Manifest.lookup('application.js'), 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application' %>
  </head>

  <body>
    <h1>此页面继承自layouts</h1>
    <%= yield %>
  </body>
</html>
